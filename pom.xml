							</Implementation-Build>
							<pomVersion>${pom.version}</pomVersion>
							<artifactId>${pom.artifactId}</artifactId>
							<groupId>${pom.groupId}</groupId>
							<!--  <basedir>${basedir}</basedir>  -->
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>


			<!-- plugin>
				<groupId>org.apache.maven.plugins</groupId>
